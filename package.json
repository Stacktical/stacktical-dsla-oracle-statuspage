{
  "name": "stacktical-dsla-oracle-statuspage",
  "version": "1.0.0",
  "description": "An OpenAI Public StatusPage Chainlink external adapter for DSLA Protocol.",
  "main": "build/src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "DEBUG=develop NODE_ENV=development concurrently \"tsc -w\" \"nodemon --watch ./build/ --exec npm run start\"",
    "start": "npx functions-framework --source=build/src/ --target=stacktical-dsla-oracle-statuspage --port 6060",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "lint": "gts lint",
    "clean": "gts clean",
    "build": "tsc src/*.ts --outDir build/src/",
    "fix": "gts fix",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacktical/stacktical-dsla-oracle-statuspage.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stacktical/stacktical-dsla-oracle-statuspage/issues"
  },
  "homepage": "https://github.com/stacktical/stacktical-dsla-oracle-statuspage#readme",
  "keywords": [
    "openai",
    "dsla",
    "statuspage",
    "sla"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "joi": "^17.6.0",
    "moment": "^2.29.4",
    "node-fetch": "^2.6.7",
    "web3": "^1.10.0",
    "web3-utils": "^1.10.0"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.1.2",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^14.11.2",
    "@types/web3": "^1.2.2",
    "concurrently": "^7.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gts": "^3.1.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.0.3"
  },
  "watch": {
    "start": "*.ts"
  }
}